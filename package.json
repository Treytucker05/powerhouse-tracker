{
  "name": "powerhouse-rp-toolkit",
  "type": "module",
  "version": "1.0.0",
  "description": "Renaissance Periodization Training Toolkit for PowerHouseATX",
  "scripts": {
    "dev": "parcel index.html --no-cache --open",
    "lint": "eslint \"js/**\"",
    "format": "prettier --write .",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "build": "parcel build index.html --dist-dir dist --public-url ./",
    "audit": "node scripts/collect-ui-inventory.js && node scripts/convert-inventory-to-md.js buttons.json && node scripts/report-missing-handlers.js"
  },
  "jest": {
    "testEnvironment": "jsdom"
  },
  "keywords": [
    "fitness",
    "renaissance periodization",
    "training",
    "workout"
  ],
  "author": "PowerHouseATX",
  "license": "MIT",
  "devDependencies": {
    "@parcel/packager-raw-url": "^2.15.2",
    "@parcel/transformer-webmanifest": "^2.15.2",
    "buffer": "^6.0.3",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.5",
    "jest": "^30.0.0",
    "jest-environment-jsdom": "^30.0.0",
    "parcel": "^2.15.2",
    "playwright": "^1.53.1",
    "prettier": "^3.5.3"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.50.0",
    "chart.js": "^4.5.0",
    "clipboardy": "^4.0.0"
  },
  "targets": {
    "default": {
      "sourceMap": true,
      "engines": {
        "browsers": "> 0.5%, not dead"
      }
    }
  }
}
